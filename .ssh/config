Host stage stage.grokker.com
     User ubuntu
     ForwardAgent yes
     StrictHostKeyChecking no
     UserKnownHostsFile /dev/null
     HostName stage.grokker.com
     IdentityFile ~/.ssh/grokker_east_rsa

Host prod grokker.com
     User ubuntu
     ForwardAgent yes
     StrictHostKeyChecking no
     UserKnownHostsFile /dev/null
     HostName grokker.com
     IdentityFile ~/.ssh/grokker_east_rsa

Host *amazonaws.com
     User ubuntu
     ForwardAgent yes
     StrictHostKeyChecking no
     UserKnownHostsFile /dev/null
     IdentityFile ~/.ssh/grokker_east_rsa

Host utility utility.gr0k.us
     User ubuntu
     ForwardAgent yes
     StrictHostKeyChecking no
     UserKnownHostsFile /dev/null
     HostName utility.gr0k.us
     IdentityFile ~/.ssh/grokker_east_rsa

Host *gr0k.us
     User ubuntu
     ForwardAgent yes
     StrictHostKeyChecking no
     UserKnownHostsFile /dev/null
     IdentityFile ~/.ssh/grokker_east_rsa

Host db3
     User ubuntu
     ForwardAgent yes
     StrictHostKeyChecking no
     UserKnownHostsFile /dev/null
     IdentityFile ~/.ssh/grokker_east_rsa
     ProxyCommand ssh ubuntu@utility nc %h %p

Host *
     ForwardAgent yes
     Protocol 2
     ForwardX11 no
     ServerAliveInterval 30
